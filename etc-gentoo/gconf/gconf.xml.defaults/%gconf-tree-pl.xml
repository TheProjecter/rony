<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="notification-daemon">
				<entry name="theme">
					<local_schema short_desc="Obecny motyw">
						<longdesc>Motyw używany podczas wyświetlania powiadomień.</longdesc>
					</local_schema>
				</entry>
				<entry name="popup_location">
					<local_schema short_desc="Położenie wyskakującego okna">
						<longdesc>Domyślne położenie wyskakującego okna na obszarze roboczym dla stosu powiadomień. Dozwolone wartości: &quot;top_left&quot;,&quot;top_right&quot;,&quot;bottom_left&quot; i &quot;bottom_right&quot;</longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
