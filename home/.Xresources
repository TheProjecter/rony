! ----------------------------------------------------------------------------
! file:     ~/.Xdefaults
! author:   Rony Yeung
! modified: Jan 2010
! vim:enc=utf-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
! ----------------------------------------------------------------------------

*.jumpScroll: true
*.multiScroll: true


! http://freedesktop.org/wiki/ScreenFontSettings
Xft.lcdfilter:  lcddefault
Xft.autohint:   0
Xft*antialias:  true
Xft*dpi:        121,120
Xft*hinting:	true
Xft*hintStyle:	hintslight  ! hint(none, slight, medium, full)
Xft*rgba:	rgb
Xft*lcdfilter:  lcddefault  ! lcd(none, default, light, legacy)

! URxvt.background:       black
! URxvt.foreground:       white
! URxvt.cursorColor:      green
! URxvt.font:             xft:Terminus:pixelsize=14:antialias=false
! URxvt.boldFont:         xft:Terminus:bold:pixelsize=14:antialias=false
! URxvt*font:             x:-misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1
! URxvt.boldFont:         -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1
! URxvt.perl-ext-common:  default,tabbed
! URxvt*perl-ext-common:      default,matcher,clipboard

! URxvt*loginShell: true
URxvt*saveLines: 4095
URxvt.buffered: true           ! false with fast onboard nvidia
URxvt*scrollBar: false
URxvt*internalBorder: 0
URxvt*externalBorder: 0
! URxvt*scrollstyle: plain
! URxvt*scrollBar_right: true
! URxvt*termName: rxvt

! Bitmap font iso10646
!
! URxvt*font: -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1

! Terminus font
!
! !URxvt*font:     Terminus:pixelsize=14, -wenquanyi-wenquanyi bitmap song-medium-r-normal--16-160-75-75-p-80-iso10646-1
! !URxvt*boldFont: Terminus:style=Bold:pixelsize=14, -arphic-ar pl uming cn-light-r-normal--0-0-0-0-c-0-iso10646-1
! !URxvt*font:     -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1, -wenquanyi-wenquanyi bitmap song-medium-r-normal--16-160-75-75-p-80-iso10646-1
! URxvt*font:         -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1, -*-wenquanyi bitmap song-medium-*-*--22-*-*-*-*-*-*-*
! URxvt*boldFont:     -xos4-terminus-bold-r-normal--14-140-72-72-c-80-iso10646-1, -wenquanyi-wenquanyi bitmap song-bold-r-normal--16-160-75-75-p-80-iso10646-1

! Good font
!
! URxvt*font: xft:DejaVu Sans Mono-8
!
URxvt*font:             xft:DejaVu Sans Mono:antialias=true:pixelsize=14
URxvt*boldFont:         xft:DejaVu Sans Mono:style=bold:antialias=true:pixelsize=14
URxvt*italicFont:       xft:DejaVu Sans Mono:style=italic:antialias=true:pixelsize=14
URxvt*boldItalicFont:   xft:DejaVu Sans Mono:style=bolditalic:antialias=true:pixelsize=14

!Envy Code R
!xft:envy code r:medium:pixelsize=12


!urxvt.keysym.M-S-v: command:/usr/bin/xclip -selection clipboard -o

URxvt*background: #000000
URxvt*foreground: #ffffff
URxvt.underlineColor:   yellow
URxvt*cursorColor: magenta
URxvt*color0: #000000
URxvt*color1: #9e1828
