<menu id="lockscreen-menu" label="lock screen">
	<item label="Now">
		<action name="Execute">
			<command>xlock -mode blank</command>
		</action>
	</item>
	<item label="Cancel">
		<action name="Execute">
			<command>kill -15 $(pidof xautolock)</command>
		</action>
	</item>
</menu>

