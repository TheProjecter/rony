<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  <item label="lxterm">
    <action name="Execute">
      <command>lxterminal</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="xterm">
    <action name="Execute">
      <command>xterm -fg wheat -bg black -fn -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
